#include <stdio.h>

// Fonction récursive pour calculer Fibonacci
int fib(int n) {
    if (n == 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    }

    return fib(n - 1) + fib(n - 2);
}

int main() {
    int n = 5;  // Exemple d'entrée pour la fonction Fibonacci

    // Appel de la fonction Fibonacci
    int result = fib(n);

    // Affiche le résultat
    printf("Fibonacci de %d : %d\n", n, result);

    return 0;
}
